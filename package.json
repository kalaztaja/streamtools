    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.0"
  },
  "scripts": {
    "start": "parcel watch src/casters.html src/scorebar.html src/left-banner.html -d graphics --public-url ./",
